# Primary LLM - Groq (https://console.groq.com/keys)
GROQ_API_KEY=your_groq_api_key_here
LLM_MODEL=groq:llama-3.1-8b-instant

# Fallback LLM - Cerebras (https://cloud.cerebras.ai/)
# Used automatically when primary model returns 429 (rate limit)
CEREBRAS_API_KEY=your_cerebras_api_key_here
LLM_FALLBACK_MODEL=openai:llama-3.3-70b
OPENAI_BASE_URL=https://api.cerebras.ai/v1

# Observability (https://logfire.pydantic.dev/)
LOGFIRE_TOKEN=
